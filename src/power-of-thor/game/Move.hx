package game;

/**
  * Enum Model of all possible moves which can be made by the player
  */
enum Move {
	//- Up
	North;

	//- Down
	South;

	//- Right
	East;

	//- Left
	West;

	//- Diagonally Up+Right
	NorthEast;

	//- Diagonally Down+Right
	SouthEast;

	//- Diagonally Up+Left
	NorthWest;

	//- Diagonally Down+Left
	SouthWest;
}
